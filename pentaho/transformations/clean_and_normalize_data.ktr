    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Business Rule Validations
var is_positive_prices = (
    open_price > 0 &amp;&amp;
    high_price > 0 &amp;&amp;
    low_price > 0 &amp;&amp;
    close_price > 0
);

var is_valid_ohlc_relationship = (
    high_price >= low_price &amp;&amp;
    high_price >= open_price &amp;&amp;
    high_price >= close_price &amp;&amp;
    low_price &lt;= open_price &amp;&amp;
    low_price &lt;= close_price
);

var is_valid_volume = (volume >= 0);

// Temporarily skip date range validation
var is_valid_date_range = true;

// Overall validation
var is_valid_row = (
    is_valid_symbol &amp;&amp;
    is_valid_date &amp;&amp;
    is_valid_price_format &amp;&amp;
    is_positive_prices &amp;&amp;
    is_valid_ohlc_relationship &amp;&amp;
    is_valid_volume &amp;&amp;
    is_valid_date_range
);

// Validation message
var validation_message = "";
if (!is_valid_symbol) validation_message += "Invalid symbol format; ";
if (!is_positive_prices) validation_message += "Negative/zero prices; ";
if (!is_valid_ohlc_relationship) validation_message += "Invalid OHLC relationship; ";
if (!is_valid_volume) validation_message += "Invalid volume; ";
if (!is_valid_date_range) validation_message += "Date out of range; ";

if (validation_message === "") {
    validation_message = null;
}</jsScript_script>
      </jsScript>
    </jsScripts>
