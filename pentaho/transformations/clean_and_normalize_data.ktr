      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Regex patterns
var symbol_pattern = /^[A-Z]{1,5}$/;
var date_pattern = /^\d{4}-\d{2}-\d{2}$/;
var price_pattern = /^\d+\.?\d*$/;

var symbol_str = symbol;
var date_str = price_date;
var close_str = close_price;

// Validate with regex
var is_valid_symbol = symbol_pattern.test(symbol_str);
var is_valid_date = date_pattern.test(date_str);
var is_valid_price_format = price_pattern.test(close_str) &amp;&amp; parseFloat(close_str) > 0;

// Validate date range (while still a string)
var is_valid_date_range = false;
if (is_valid_date) {
    var year = parseInt(date_str.substring(0, 4));
    var month = parseInt(date_str.substring(5, 7));
    var day = parseInt(date_str.substring(8, 10));
    
    is_valid_date_range = (
        year >= 2020 &amp;&amp; year &lt;= 2026 &amp;&amp;
        month >= 1 &amp;&amp; month &lt;= 12 &amp;&amp;
        day >= 1 &amp;&amp; day &lt;= 31
    );
}</jsScript_script>
