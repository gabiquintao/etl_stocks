    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Calculate price changes
var price_change = close_price - open_price;
var price_change_pct = ((close_price - open_price) / open_price) * 100;

// Calculate daily return (vs previous day close)
var daily_return = null;
if (previous_close != null &amp;&amp; previous_close > 0) {
    daily_return = ((close_price - previous_close) / previous_close) * 100;
}

// Calculate intraday range
var intraday_range = high_price - low_price;
var intraday_range_pct = ((high_price - low_price) / open_price) * 100;

// Round to 4 decimal places
price_change = Math.round(price_change * 10000) / 10000;
price_change_pct = Math.round(price_change_pct * 10000) / 10000;
if (daily_return != null) {
    daily_return = Math.round(daily_return * 10000) / 10000;
}
intraday_range = Math.round(intraday_range * 10000) / 10000;
intraday_range_pct = Math.round(intraday_range_pct * 10000) / 10000;</jsScript_script>
      </jsScript>
